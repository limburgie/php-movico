<?xml version="1.0"?>
<!DOCTYPE view SYSTEM "http://php-movico.googlecode.com/svn/trunk/php-movico/lib/component/dtd/view.dtd">
<template>
	<css src="test.css"/>
	<h1>Movico Sample Browser</h1>
	<panelGrid columns="2">
		<form id="nav">
			<h2>Bean Examples</h2>
			<ul>
				<li><commandLink action="beans/request/hello" value="HelloBean (request)"/></li>
				<li><commandLink action="beans/session/login" value="LoginBean (session)"/></li>
				<li><commandLink action="beans/application/counter" value="CounterBean (application)"/></li>
			</ul>
			<h2>Service Examples</h2>
			<ul>
				<li><commandLink action="services/onetomany/teams" value="One-to-many relationship"/></li>
				<li><commandLink action="services/manytomany/classes" value="Many-to-many relationship"/></li>
			</ul>
			<h2>Component Examples</h2>
			<ul>
				<li><commandLink action="components/datatable/users" value="DataTable"/></li>
				<li><commandLink action="components/listbox/countries" value="ListBox"/></li>
				<li><commandLink action="components/fileupload/upload_file" value="File Upload"/></li>
			</ul>
			<ajaxLoading/>
		</form>	
		<div id="main">
			<h2><insert name="title"/></h2>
			<insert name="body"/>
		</div>
	</panelGrid>
</template>