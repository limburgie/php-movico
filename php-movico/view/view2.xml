<?xml version="1.0"?>
<!DOCTYPE view SYSTEM "../lib/component/dtd/view.dtd">
<view>
	<dataTable var="user" value="HelloBean.users">
		<column>
			<colHeader>
				<outputText value="Voornaam"/>
			</colHeader>
			<outputText value="user.firstName"/>
		</column>
		<column>
			<colHeader>
				<outputText value="Achternaam"/>
