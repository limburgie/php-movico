<?xml version="1.0"?>
<!DOCTYPE service-builder SYSTEM "../lib/servicebuilder/dtd/service.dtd">
<service-builder>
	<entity name="Movie" table="movies_movie">
		<column name="id" type="int" primary="true" auto-increment="true"/>
		<column name="title" type="String" size="50"/>
		<column name="altTitle" type="String" size="50"/>
		<column name="creationDate" type="Date"/>
		<column name="genre" type="String" size="20"/>
		<column name="year" type="int"/>
		<column name="director" type="String" size="50"/>
		<column name="actors" type="String" size="100"/>
		<column name="bought" type="boolean"/>
		<column name="default" type="boolean"/>
	</entity>
	
	<entity name="MovieScoring">
		<column name="id" type="int" primary="true" auto-increment="true"/>
		<column name="userId" type="int"/>
		<column name="movieId" type="int"/>
		<column name="score" type="float"/>
	</entity>
	
	<entity name="User">
		<column name="id" type="int" primary="true" auto-increment="true"/>
		<column name="username" type="String" size="20"/>
		<column name="password" type="String" size="20"/>
	</entity>
</service-builder>